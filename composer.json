{
    "name": "nitsan/ns-feedback",
    "type": "typo3-cms-extension",
    "description": "The TYPO3 Feedback Extension is a great tool for gathering feedback from visitors or customers on your website. With the All In One TYPO3 Feedback extension, website admin can easily add feedback forms in various styles to their website, allowing them to collect valuable insights from their visitors.",
    "license": "GPL-2.0+",
    "keywords": [
        "TYPO3 CMS",
        "feedback",
        "servey",
        "extension",
        "CMS"
    ],    
    "authors": [
        {
            "name": "NITSAN Technologies Pvt Ltd",
            "email": "info@nitsan.in",
            "role": "Developer",
            "homepage": "http://www.nitsan.in/"
        }
    ],
    "require": {
       "typo3/cms-core": "^12 || ^13"
    },
    "autoload": {
        "psr-4": {
            "NITSAN\\NsFeedback\\": "Classes"
        }
    },    
    "replace": {
        "typo3-ter/ns-feedback": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor/",
        "bin-dir": ".Build/bin/"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/public",
            "extension-key": "ns_feedback"
        }
    }
}
